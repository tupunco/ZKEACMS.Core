@{
    Style.Reqiured("bootStrap").AtHead();
}

<!DOCTYPE html>

<html style="height:100%">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @StyleAtHead()
    @ScriptAtHead()
</head>
